// password
// you can also change the password by yourself

#include <iostream>
using namespace std;
int main(){
    int n;
    cout << "enter the password: ";
    cin >> n;
    if (n % 5 != 0){
        for (int i=1;i<=5;i++){
            if (n != 12345){    // you can also change the password whatever you want
                for (int i=1;i<=5;i++){
                    cout << "enter the password again: ";
                    cin >> n;
                    if (n == 12345){
                        cout << "welcome" << endl;
                        break;
                    }
                    break;
                }
            }
            break;
        }
    }
    else if (n == 12345){  // you can change 12345  to any password you want
        cout << "welcome" << endl;
    }
}
